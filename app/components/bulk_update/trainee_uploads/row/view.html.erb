<tr class="govuk-table__row">
  <td class="govuk-table__cell">
    <% if upload.cancelled? %>
      <%= created_at %>
    <% else %>
      <%= govuk_link_to created_at, upload_path %>
    <% end %>
  </td>
  <td class="govuk-table__cell">
    <%= filename %>
  </td>
  <td class="govuk-table__cell">
    <%= status %>
  </td>
</tr>
