<%= render MappableSummary::View.new(
  trainee: trainee,
  title: "Withdrawal details",
  rows: rows,
  editable: editable,
  has_errors: false,
  render_list_only: render_list_only,
) do |component| %>
  <% component.with_header_actions do %>
    <% if undo_withdrawal %>
      <%= govuk_link_to "Undo withdrawal", trainee_undo_withdrawal_path(trainee) %>
    <% end %>
  <% end %>
<% end %>
