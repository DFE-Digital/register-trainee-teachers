<%= render MappableSummary::View.new(
  trainee: trainee,
  title: summary_title,
  rows: withdrawal_rows,
  editable: editable,
  has_errors: has_errors
) do |component| %>
  <% component.header_actions do %>
    <% if show_link %>
      <%= govuk_link_to "Undo withdrawal", trainee_undo_withdrawal_path(trainee) %>
    <% end %>
  <% end %>
<% end %>
