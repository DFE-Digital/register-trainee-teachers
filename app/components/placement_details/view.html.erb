<%= render MappableSummary::View.new(
  trainee: trainee,
  title: summary_title,
  rows: rows,
  editable: false,
  has_errors: has_errors,
  ) do |component| %>
  <% component.with_header_actions do %>
    <% if editable %>
      <%= govuk_link_to("Manage placements", trainee_placements_confirm_path(trainee)) %>
    <% end %>
  <% end %>
<% end %>
