<div class="govuk-form-group">
  <label class="govuk-label"><%= @trainee.full_name %></label>

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds-from-desktop">
      <%= register_form_with(model: @trainee, url: trainee_accredited_providers_confirmations_path(trainee_id: @trainee.id)) do |f| %>

        <%= f.govuk_error_summary %>

        <h1 class="govuk-heading-l">Check change of accredited provider details</h1>

        <p></p>

        <%= f.govuk_submit "Update record" %>
      <% end %>
    </div>
  </div>
</div>

