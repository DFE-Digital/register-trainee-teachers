<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds-from-desktop">
    <%= register_form_with(
      model: @lead_partner_form,
      url: user_lead_partners_path(@user, query: params[:query]),
      method: :post,
      local: true,
    ) do |form| %>
      <% render (@lead_partner_search.lead_partners.empty? ? "no_results" : "results"), form: form %>
    <% end %>
  </div>
</div>
