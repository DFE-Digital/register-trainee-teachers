{
  "env_config": "qa",
  "paas_app_environment": "qa",
  "key_vault_name": "s189t01-rtt-qa-kv",
  "key_vault_app_secret_name": "REGISTER-APP-SECRETS-QA",
  "key_vault_infra_secret_name": "BAT-INFRA-SECRETS-QA",
  "cluster": "test",
  "namespace": "bat-qa",
  "db_sslmode": "prefer",
  "postgres_create_servicename_db": true,
  "postgres_extensions": "PGCRYPTO,BTREE_GIST,CITEXT,PG_TRGM",
  "postgres_version": "13",
  "main_app": {
    "main": {
      "startup_command": ["/bin/sh", "-c", "bundle exec rails db:migrate && bundle exec rails server -b 0.0.0.0"],
      "replicas": 2,
      "memory_max": "1Gi"
    }
  },
  "worker_apps": {
    "worker": {
      "startup_command": ["/bin/sh", "-c", "bundle exec sidekiq -C config/sidekiq.yml"],
      "replicas": 1,
      "memory_max": "1Gi"
    }
  },
  "deploy_temp_data_storage_account": true,
  "azure_tempdata_storage_account_name": "s189t01registerqatmp",
  "azure_resource_group_name": "s189t01-rtt-qa-rg",
  "gov_uk_host_names": [
    "qa2.register-trainee-teachers.service.gov.uk",
    "qa2.register-trainee-teachers.education.gov.uk"
  ],
  "enable_alerting": true,
  "pg_actiongroup_name": "s189t01-rtt-test-ag",
  "pg_actiongroup_rg": "s189t01-rtt-qa-rg",
  "statuscake_alerts": {
    "alert": {
      "website_name": "register-aks-qa",
      "website_url": "https://qa2.register-trainee-teachers.service.gov.uk/ping",
      "check_rate": 60,
      "contact_group": [282783]
    }
  }
}
