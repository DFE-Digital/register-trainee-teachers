{
  "env_config": "review",
  "paas_app_environment": "review",
  "key_vault_name": "s189d01-rtt-rv-kv",
  "key_vault_app_secret_name": "REGISTER-APP-SECRETS-QA",
  "key_vault_infra_secret_name": "BAT-INFRA-SECRETS-QA",
  "namespace": "development",
  "db_sslmode": "prefer",
  "deploy_azure_backing_services": true,
  "enable_monitoring": true,
  "main_app": {
    "main": {
      "startup_command": ["/bin/sh", "-c", "bundle exec rails db:migrate && bundle exec rails server -b 0.0.0.0"],
      "probe_path": "/ping",
      "replicas": 1,
      "memory_max": "1Gi"
    }
  },
  "worker_apps": {
    "worker": {
      "startup_command": ["/bin/sh", "-c", "bundle exec sidekiq -C config/sidekiq.yml"],
      "replicas": 1,
      "memory_max": "1Gi"
    }
  },
  "deploy_temp_data_storage_account": false
}
