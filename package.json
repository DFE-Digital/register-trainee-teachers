{
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "js:lint": "standard",
    "js:lint:fix": "standard --fix",
    "scss:lint": "stylelint 'app/assets/stylesheets/**/*.scss'",
    "scss:lint:fix": "stylelint 'app/assets/stylesheets/**/*.scss' --fix",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --coverage",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds",
    "build:css": "sass ./app/assets/stylesheets/application.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules --load-path=. --quiet-deps"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^1.8.0",
    "accessible-autocomplete": "^2.0.3",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.33.2",
    "esbuild": "^0.19.5",
    "govuk-country-and-territory-autocomplete": "^1.0.2",
    "govuk-frontend": "^4.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jquery": "^3.7.1",
    "rails-ujs": "^5.2.8",
    "sass": "^1.69.5",
    "serialize-javascript": "^6.0.1",
    "set-value": "^4.1.0",
    "turbolinks": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.23.3",
    "@babel/plugin-transform-regenerator": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-macros": "^3.1.0",
    "jest": "^29.7.0",
    "standard": "^17.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-gds": "^1.1.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-scss": "^5.3.1"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-gds/scss"
  },
  "license": "MIT"
}
